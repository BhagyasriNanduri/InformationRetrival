   "source": [
    "from nltk.stem.snowball import SnowballStemmer\n",
    "from nltk.tokenize import sent_tokenize, word_tokenize\n",
    "from nltk.corpus import stopwords\n",
    "from nltk.tokenize import TweetTokenizer\n",
    "import string\n",
    "unstained_text= []\n",
    "for j in consumer_data['title']:\n",
    "  \n",
    "    unstain_d = re.sub(r'[^\\x00-\\x7F]+', '', j)\n",
    "    unstain_d  = re.sub(r'@\\w+', '',  unstain_d)\n",
    "    unstain_d =   unstain_d.lower()\n",
    "    unstain_d  = re.sub(r'[%s]' % re.escape(string.punctuation), '',  unstain_d) \n",
    "    unstain_d  = re.sub(r'[0-9]', '',  unstain_d)\n",
    "    unstain_d = re.sub(r'\\s{2,}', '',  unstain_d)\n",
    "    con_str= re.compile(r'\\b(' + r'|'.join(stopwords.words('english')) + r')\\b\\s*')\n",
    "    text= con_str.sub('',   unstain_d)\n",
    "    unstained_text.append(text)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "de301b15",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Stemmer\n",
    "from nltk.stem.snowball import SnowballStemmer\n",
    "s_stripper = SnowballStemmer(\"english\")\n",
    "def s_strip(text):\n",
    "    count=0\n",
    "    s_strip=[]\n",
    "    K1=[]\n",
    "    for i in text:\n",
    "        s=[]\n",
    "        tnz=word_tokenize(i)\n",
    "        tnz\n",
    "        s_strip=[]\n",
    "        for word in tnz:\n",
    "            s_strip.append(s_stripper.stem(word))\n",
    "        s=' '.join(s_strip)\n",
    "        K1.insert(count,s)\n",
    "        count=count+1\n",
    "        \n",
    "    return K1"
   ]
  },
