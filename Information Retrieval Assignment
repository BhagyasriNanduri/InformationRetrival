   "source": [
    "#Taking input from user\n",
    "clustering_test = input(\"Enter a keyword to know cluster: \")\n",
    "\n",
    "\n",
    "wd_t = word_tokenize(clustering_test)\n",
    "x = \"\"\n",
    "for w in wd_t :\n",
    "    if w not in stopwords.words(\"english\"):\n",
    "        x += cl_st.stem(w) + \" \"\n",
    "print(x)   \n",
    "\n",
    "Y = tfid .transform([x])\n",
    "res = k_means_model.predict(Y)\n",
    "print(\"cluster is: \", res)"
