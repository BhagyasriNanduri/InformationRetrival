    "def consumer_profile():\n",
    "    if os.path.exists(\"data.json\"):\n",
    "        os.remove(\"data.json\")\n",
    "    sc_accounting='https://pureportal.coventry.ac.uk/en/organisations/school-of-economics-finance-and-accounting'\n",
    "    sc_accounting= sc_accounting +'/'+'publications'\n",
    "    s=[1]\n",
    "    media_type={}\n",
    "    while len(s)!=2:\n",
    "        rq = requests.get(sc_accounting)\n",
    "        media_type=consumer(sc_accounting,media_type)\n",
    "        s_bu = BeautifulSoup(rq.content, 'html.parser')\n",
    "        n_page = s_bu.find_all(\"li\", class_= \"next\")\n",
    "        s=str(n_page)\n",
    "        if len(s)!=2:\n",
    "            s=s.split()[6]\n",
    "            S1='https://pureportal.coventry.ac.uk'\n",
    "            consumer_url = re.findall('\"([^\"]*)\"', s)\n",
    "            consumer_url=consumer_url[0]\n",
    "            print(consumer_url)\n",
    "            sc_accounting=S1+consumer_url\n",
    "            print(sc_accounting)\n",
    "    return(media_type)"
   ]
