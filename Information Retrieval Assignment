    "def consumer(consumer_url,media_type):\n",
    "    \n",
    "    rq = requests.get(consumer_url)\n",
    "    print(consumer_url)\n",
    "    s_bu= BeautifulSoup(rq.content, 'html.parser')\n",
    "    f_lin = s_bu.find_all(\"div\",{'class':\"result-container\"})\n",
    "    count=0\n",
    "    for details in f_lin:\n",
    "\n",
    "            attribute_=details.find(\"a\",{'rel':[\"ContributionToJournal\",\"ContributionToConference\",\\\n",
    "                                          \"ContributionToBookAnthology\",\"BookAnthology\",\"ContributionToPeriodical\",\\\n",
    "                                          \"OtherContribution\",\"Thesis\",\"NonTextual\",\"Patent\",\"WorkingPaper\"]})\n",
    "            \n",
    "            try:\n",
    "                review = attribute_.get_text()\n",
    "                review_link = attribute_['href']\n",
    "            except:\n",
    "                 review = 'unknown'\n",
    "            \n",
    "            client = details.find(\"a\",{'rel':\"Person\"})\n",
    "            try:\n",
    "                year = details.find(\"span\",{'class':\"date\"})\n",
    "                year =  year.get_text()\n",
    "            except:\n",
    "                year = 'unknown'\n",
    "            try:\n",
    "                 client_link = person['href']\n",
    "            except:\n",
    "                 client_link ='unknown'\n",
    "            reviewers=[]\n",
    "            if client!=None:\n",
    "                for det in client:\n",
    "                    reviewers.append(det.get_text())\n",
    "            reviewers1 =''\n",
    "            for x in reviewers:\n",
    "                reviewers1+=' '+x\n",
    "            media_type = {'publicationlink': review_link,\n",
    "                       'title': review,\n",
    "                       'authors':reviewers1,\n",
    "                       'personlink':client_link,\n",
    "                   'year':year}\n",
    "            \n",
    "            with open('data.json', 'a+') as result:\n",
    "                    result.write(json.dumps(media_type))\n",
    "                    result.write('\\n')\n",
    "                    result.close()\n",
    "        "
   ]
  },
