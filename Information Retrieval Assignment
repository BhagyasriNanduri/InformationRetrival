    "#Preprocessing\n",
    "from nltk.stem.porter import PorterStemmer\n",
    "cl_st = PorterStemmer()\n",
    "data_clean = []\n",
    "for i in text:\n",
    "    wod_tkns = word_tokenize(i.lower())\n",
    "    n = \"\"\n",
    "    for w in wod_tkns:\n",
    "        if w not in stopwords.words(\"english\"):\n",
    "            if (w.isalpha()):\n",
    "                n += cl_st.stem(w) + \" \"\n",
    "    data_clean.append(n)\n",
    "print(data_clean)"
   ]
